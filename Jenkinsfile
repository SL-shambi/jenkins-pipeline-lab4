pipeline {
    agent any
    stages {
        stage('Build'){
            steps {
                sh "echo hello"
              }
    stages {
        stage('Test'){
            steps {
                sh "ls -la"
                sh "touch testfile.txt"
              }
    stages {
        stage('Deploy'){
            steps {
                sh "pwd"
                sh "rm -f testfile.txt"
              }
        }
    }
